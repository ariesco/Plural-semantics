    = (fmod 'SGRAMMAR is
         including 'QID-LIST .
         including 'PLURAL-SIGN .
         sorts none .
         none
         op 'token : 'Qid -> '@Token@
              [special(
                (id-hook('Bubble, '1 '1)
                 op-hook('qidSymbol, '<Qids>, nil, 'Qid)
                 id-hook('Exclude, '`[ '`] '< 'to ': '`, '. '`( '`)  '| )))] .
