  if varInTerm(V, T) /\
     V =/= T /\
     URL' := add2result(V -> V | T, URL) .
 ceq [ubind] : q-unify((V ==? T) U UP, URL) = q-unify(UP, URL')
  if not varInTerm(V, T) /\
